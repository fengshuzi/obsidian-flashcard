{
    "name": "obsidian-flashcard",
    "version": "1.0.0",
    "description": "Flashcard review plugin with Logseq #flashcard format support.",
    "main": "main.js",
    "scripts": {
        "build": "node esbuild.config.mjs production",
        "dev": "node esbuild.config.mjs",
        "deploy": "node deploy.mjs",
        "build-deploy": "npm run build && node deploy.mjs",
        "format": "npm run prettier -- --write . && npm run eslint -- src/ tests/ --fix",
        "lint": "npm run prettier -- --check . && npm run eslint -- src/ tests/",
        "test": "npm run jest",
        "validate": "concurrently --kill-others-on-fail -g -p \"[{name}]\" -n \"format,lint,test\" \"npm:format\" \"npm:lint\" \"npm:test\"",
        "precommit-msg": "echo 'Pre-commit checks...'",
        "changelog": "npm run auto-changelog -- --template=compact --package && npm run prettier -- --write docs/docs/changelog.md",
        "prettier": "prettier",
        "eslint": "eslint",
        "jest": "jest",
        "auto-changelog": "auto-changelog",
        "release": "node release.mjs"
    },
    "keywords": [
        "obsidian",
        "spaced repetition",
        "flashcard",
        "flashcards",
        "logseq"
    ],
    "author": "Stephen Mwangi",
    "license": "MIT",
    "devDependencies": {
        "@eslint/js": "^9.39.1",
        "@types/eslint__js": "^8.42.3",
        "@types/jest": "^29.5.14",
        "@types/node": "^22.19.1",
        "@types/vhtml": "^2.2.9",
        "@typescript-eslint/eslint-plugin": "^8.48.0",
        "@typescript-eslint/parser": "^8.48.0",
        "auto-changelog": "^2.5.0",
        "builtin-modules": "^4.0.0",
        "concurrently": "^9.2.1",
        "esbuild": "^0.25.12",
        "eslint": "^9.39.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "eslint-plugin-unicorn": "^55.0.0",
        "jest": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "jest-expect-message": "^1.1.3",
        "moment": "^2.30.1",
        "obsidian": "^1.10.3",
        "pre-commit": "^1.2.2",
        "prettier": "^3.7.3",
        "ts-jest": "^29.4.5",
        "tslib": "^2.8.1",
        "typescript": "~5.5.4",
        "typescript-eslint": "^8.48.0",
        "vhtml": "^2.2.0"
    },
    "dependencies": {
        "chart.js": "^4.5.1",
        "clozecraft": "^0.4.0",
        "gridjs": "^6.2.0",
        "minimatch": "^10.1.1",
        "pagerank.js": "^1.0.2",
        "short-uuid": "^5.2.0"
    },
    "pre-commit": [
        "precommit-msg",
        "validate"
    ]
}
